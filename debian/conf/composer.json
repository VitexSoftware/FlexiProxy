{
    "name": "vitxsoftware/flexiproxy",
    "description": "Advanced Access privileges for FlexiBee",
    "version": "0.2.8",
    "type": "project",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "/var/lib/flexiproxy"
    },
    "require": {
        "php": ">= 5.5.9",
        "flexipeehp": "*",
        "ease-framework": "*",
        "league/commonmark": "^0.15.0"
    },
    "autoload": {
        "psr-4": {
            "FlexiProxy\\": "FlexiProxy/",
            "FlexiProxy\\ui\\": "FlexiProxy/ui/",
            "FlexiProxy\\plugins\\": "FlexiProxy/plugins/",
            "FlexiProxy\\plugins\\output\\": "FlexiProxy/plugins/output/",
            "FlexiProxy\\plugins\\output\\html\\": "FlexiProxy/plugins/output/html/",
            "FlexiProxy\\plugins\\output\\js\\": "FlexiProxy/plugins/output/js/",
            "FlexiProxy\\plugins\\output\\json\\": "FlexiProxy/plugins/output/json/",
            "FlexiProxy\\plugins\\input\\": "FlexiProxy/plugins/input/",
            "FlexiProxy\\plugins\\input\\json\\": "FlexiProxy/plugins/input/json/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHP",
            "options": {
                "symlink": true
            }
        }
    ]
}
